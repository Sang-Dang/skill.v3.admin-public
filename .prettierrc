{
  "arrowParens": "always",
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "jsxSingleQuote": true,
  "printWidth": 140,
  "proseWrap": "always",
  "quoteProps": "consistent",
  "semi": false,
  "singleQuote": true,
  "tabWidth": 4,
  "trailingComma": "all",
  "bracketSameLine": false,
  "useTabs": false,
  "plugins": ["prettier-plugin-tailwindcss"]
}
